{
  "manifest_version": 3,
  "name": "Clip it",
  "version": "1.0",
  "description": "Save page highlights to database",
  "permissions": ["contextMenus", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "commands": {
    "open-clips": {
      "suggested_key": {
        "default": "Ctrl+Shift+K"
      },
      "description": "Open Clips Page"
    }
  }
}
